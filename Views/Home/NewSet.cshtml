@model List<LasFiszkas.Models.Fish>
@{
    ViewBag.Title = "NewSet";
}

<main>
    @using (Html.BeginForm("NewSet", "Home"))
    {
        <table class="newFish">
            @for (int i = 0; i < 3; i++)
            {
                <tr>
                    <td>Przód:</td>
                    <td>@Html.TextBoxFor(f => f[i].PlContent)</td>
                    <td>@Html.ValidationMessageFor(f => f[i].PlContent)</td>

                    <td>Tył:</td>
                    <td>@Html.TextBoxFor(f => f[i].EspContent)</td>
                    <td>@Html.ValidationMessageFor(f => f[i].EspContent)</td>
                </tr>
            }
        </table>
        <input type="submit" value="Dodaj" class="guessButton"/>
    }
</main>
